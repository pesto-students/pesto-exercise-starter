{
  "extends": "airbnb",
  "env": {
    "jest": true
  },
  "rules": {
    "no-restricted-syntax": 0,
    "arrow-parens": 0,
    "import/prefer-default-export": 0,
    "comma-dangle": 0,
    "no-plusplus": 0,
    "one-var-declaration-per-line": 0,
    "no-param-reassign": 0,
    "implicit-arrow-linebreak": 0,
    "object-curly-newline": 0,
    "one-var": 0
  }
}
